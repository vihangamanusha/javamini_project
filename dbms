CREATE DATABASE Tecmis;

////////////////////////////////////////////////////

CREATE TABLE USER(
    NIC INT AUTO_INCREMENT,
    First_Name VARCHAR(50),
    Last_Name VARCHAR(50),
    Role VARCHAR(15),
    Phone_Number INT(10),
    Email VARCHAR(100),
    Password VARCHAR(15),
    PRIMARY KEY(NIC)
    
);

