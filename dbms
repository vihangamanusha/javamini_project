/////////////////////////////////////////////////////////////////////////


CREATE TABLE Medical(
    Medical_id CHAR(10),
    Course_code CHAR(10),
    Student_id CHAR(10),
    TO_id CHAR(10),
    Submission_date VARCHAR(15),
    Status VARCHAR(15),
    Start_date VARCHAR(15),
    End_date VARCHAR(15),
    Reason VARCHAR(100),
    FOREIGN KEY (Course_code) REFERENCES Course_unit(Course_code),
    FOREIGN KEY (Student_id) REFERENCES Student(Student_id),
    FOREIGN KEY (TO_id) REFERENCES Technical_officer(TO_id)
    

);


