//////////////////////////////////////////////////////////


CREATE TABLE Course_unit(
    Lecture_id CHAR(10),
    Admin_id CHAR(10),
    Course_code CHAR(10),
    CourseName VARCHAR(100),
    Course_type VARCHAR(20),
    Theory_hours INT,
    Practical_hours INT,
    Credits INT,
    FOREIGN KEY (Lecture_id) REFERENCES Lecture(Lecture_id),
    FOREIGN KEY (Admin_id) REFERENCES Admin(Admin_id),
    PRIMARY KEY(Course_code)   
);

INSERT INTO Course_unit (Lecture_id, Admin_id, Course_code, CourseName, Course_type, Theory_hours, Practical_hours, Credits)
VALUES
('L001','A001','ICT2113', 'Business Economics', '2T+1P', 2, 1, 3),
('L002','A001','ICT2122', 'Data Structures and Algorithms', '2T', 2, 0, 2),
('L003','A001','ICT2133', 'Object Oriented Programming', '2T+1P', 2, 1, 3),
('L004','A001','ICT2142', 'Object Oriented Programming Practicum', '2P', 0, 2, 2),
('L005','A001','ICT2152', 'Object Oriented Analysis and Design', '2T', 2, 0, 2);


///////////////////////////////////////////////////////////////////


CREATE TABLE Student_course_unit(
	Student_id CHAR(10),
	Course_Code CHAR(10),
        PRIMARY KEY(Student_id,Course_Code),
        FOREIGN KEY (Student_id) REFERENCES Student(Student_id),
        FOREIGN KEY (Course_code) REFERENCES Course_unit(Course_code)
  
);

