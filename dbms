//////////////////////////////////////////////////////////

CREATE TABLE Lecturer (
    NIC INT,
    Lecturer_id CHAR(10),
    PRIMARY KEY (Lecturer_id),
    FOREIGN KEY (NIC) REFERENCES USER(NIC)
);

INSERT INTO Lecturer (NIC, Lecturer_id)
VALUES
    (1, 'L001'),
    (2, 'L002'),
    (3, 'L003'),
    (4, 'L004'),
    (5, 'L005');


////////////////////////////////////////////////////////////

CREATE TABLE Admin (
    NIC INT,
    Admin_id CHAR(10),
    PRIMARY KEY (Admin_id),
    FOREIGN KEY (NIC) REFERENCES USER(NIC)
);

INSERT INTO Admin (NIC, Admin_id)
VALUES
    (6, 'A001');


/////////////////////////////////////////////////////////

CREATE TABLE Technical_officer(
     NIC INT,
     TO_id CHAR(10),
     PRIMARY KEY(TO_id),
     FOREIGN KEY (NIC) REFERENCES USER(NIC)

);

INSERT INTO Technical_officer(NIC, TO_id)
VALUES
    (7, 'T001'),
    (8, 'T002'),
    (9, 'T003'),
    (10,'T004');

/////////////////////////////////////////////////////////

CREATE TABLE Student(
      NIC INT,
      Student_id CHAR(10),
      Timetable_id CHAR(10),
      PRIMARY KEY(Student_id),
      FOREIGN KEY (NIC) REFERENCES USER(NIC),
      FOREIGN KEY (Timetable_id) REFERENCES Timetable(Timetable_id).
);

