    (10,'T004');

/////////////////////////////////////////////////////////

CREATE TABLE Student(
      NIC INT,
      Student_id CHAR(10),
      PRIMARY KEY(Student_id),
      FOREIGN KEY (NIC) REFERENCES USER(NIC)
);

