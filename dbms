/////////////////////////////////////////////////////////////////////
CREATE TABLE Lecturer_Medical(
	Lecturer_id CHAR(10),
	Medical_id CHAR(10),
        PRIMARY KEY(Lecturer_id,Medical_id),
        FOREIGN KEY (Lecturer_id) REFERENCES Lecturer(Lecturer_id),
        FOREIGN KEY (Medical_id) REFERENCES Medical(Medical_id)
  
);

//////////////////////////////////////////////////////////

CREATE TABLE Timetable(
	Timetable_id CHAR(10),
        Admin_id CHAR(10),
        Content LONGTEXT,
        PRIMARY KEY(Timetable_id),
        FOREIGN KEY (Admin_id) REFERENCES Admin(Admin_id)
         
);
*********DATA danna

////////////////////////////////////////////////////////////

CREATE TABLE Timetable_TO_0fficer (
	Timetable_id CHAR(10),
	TO_id CHAR(10),
        PRIMARY KEY(Timetable_id,TO_id),
        FOREIGN KEY (Timetable_id) REFERENCES Timetable(Timetable_id),
        FOREIGN KEY (TO_id) REFERENCES Technical_officer(TO_id)
  
);


///////////////////////////////////////////////////

CREATE TABLE Notice(
	Notice_id CHAR(10),
        Title VARCHAR(50),
        Admin_id CHAR(10),
        PRIMARY KEY(Notice_id),
        FOREIGN KEY (Admin_id) REFERENCES Admin(Admin_id)
         
);

****DATA DANNA


/////////////////////////////////////////////////////////

CREATE TABLE Notice_Lecturer(
	Notice_id CHAR(10),
	Lecturer_id CHAR(10),
        PRIMARY KEY(Notice_id,Lecturer_id),
        FOREIGN KEY (Notice_id) REFERENCES Notice(Notice_id),
        FOREIGN KEY (Lecturer_id) REFERENCES Lecturer(Lecturer_id)
          
);

///////////////////////////////////////////////////////////


CREATE TABLE Notice_Student(
	Notice_id CHAR(10),
	Student_id CHAR(10),
        PRIMARY KEY(Notice_id,Student_id),
        FOREIGN KEY (Notice_id) REFERENCES Notice(Notice_id),
        FOREIGN KEY (Student_id) REFERENCES Student(Student_id)
          
);

//////////////////////////////////////////////////////

CREATE TABLE Notice_TO_Officer(
	Notice_id CHAR(10),
	TO_id CHAR(10),
        PRIMARY KEY(Notice_id,TO_id),
        FOREIGN KEY (Notice_id) REFERENCES Notice(Notice_id),
        FOREIGN KEY (TO_id) REFERENCES Technical_officer(TO_id)
          
);

