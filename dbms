//////////////////////////////////////////////////////////

CREATE TABLE Lecture (
    NIC INT,
    Lecture_id CHAR(10),
    PRIMARY KEY (Lecture_id),
    FOREIGN KEY (NIC) REFERENCES USER(NIC)
);

INSERT INTO Lecture (NIC, Lecture_id)
VALUES
    (1, 'L001'),
    (2, 'L002'),
    (3, 'L003'),
    (4, 'L004'),
    (5, 'L005');


////////////////////////////////////////////////////////////

CREATE TABLE Admin (
    NIC INT,
    Admin_id CHAR(10),
    PRIMARY KEY (Admin_id),
    FOREIGN KEY (NIC) REFERENCES USER(NIC)
);

INSERT INTO Lecture (NIC, Lecture_id)
VALUES
    (6, 'A001');


/////////////////////////////////////////////////////////

CREATE TABLE Technical_officer(
     NIC INT,
     TO_id CHAR(10),
     PRIMARY KEY(TO_id),
     FOREIGN KEY (NIC) REFERENCES USER(NIC)

);

INSERT INTO Technical_officer(NIC, TO_id)
VALUES
    (7, 'T001'),
    (8, 'T002'),
