////////////////////////////////////////////////////////////////////////////////



CREATE TABLE Attendance(
    Course_code CHAR(10),
    Session_Type VARCHAR(15),
    Session_date VARCHAR(50),
    Status VARCHAR(15),
    Student_id CHAR(10),
    TO_id CHAR(10),
    Medical_id CHAR(10),
    FOREIGN KEY (Course_code) REFERENCES Course_unit(Course_code),
    FOREIGN KEY (Student_id) REFERENCES Student(Student_id),
    FOREIGN KEY (TO_id) REFERENCES Technical_officer(TO_id),
    FOREIGN KEY (Medical_id) REFERENCES Medical(Medical_id)


);

